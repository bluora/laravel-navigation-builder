{
    "name": "hnhdigital-os/laravel-navigation-builder",
    "license": "MIT",
    "authors": [
        {
            "name": "Rocco Howard",
            "email": "rocco@bluora.com.au"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "hnhdigital-os/laravel-html-generator": "dev-master",
        "hnhdigital-os/laravel-number-converter": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7",        
        "codeclimate/php-test-reporter": "dev-master",
        "phpmd/phpmd" : "@stable",
        "illuminate/support": "^5.3"
    },
    "support": {
        "issues": "https://github.com/hnhdigital-os/laravel-navigation-builder/issues"
    },
    "autoload": {
        "psr-4": {
            "Bluora\\LaravelNavigationBuilder\\": "src/"
        }
    },
    "scripts": {
      "test": "composer phpunit && composer phpcs",
      "phpcs": "phpcs -p -s --standard=PSR2 ./src ./tests",
      "phpunit": "phpunit",
      "hhvm": "hhvm ./vendor/phpunit/phpunit/phpunit"
    },
    "prefer-stable": true,
    "minimum-stability" : "dev"
}
